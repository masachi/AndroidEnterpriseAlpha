<?xml version="1.0" encoding="utf-8"?>
<!--Created by baiqingyu : 学员列表-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:background="@color/color_ffffff"
              android:layout_height="match_parent">
    <org.example.androidenterprise.view.TopbarView
            android:id="@+id/topbar_student_head_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    <org.example.androidenterprise.view.CustomMeasureGridView
            android:id="@+id/gv_students_head_list"
            android:padding="@dimen/students_head_list_gv_padding"
            android:numColumns="5"
            android:stretchMode="columnWidth"
            android:verticalSpacing="@dimen/students_head_list_gv_vertical_spacing"
            android:horizontalSpacing="@dimen/students_head_list_gv_horizontal_spacing"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

    </org.example.androidenterprise.view.CustomMeasureGridView>


</LinearLayout>
